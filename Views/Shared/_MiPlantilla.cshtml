<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>@ViewBag.TitleCSS3 Inventario De Repuestos Web</title>

    <link href="~/Content/myPlantilla.css" rel="stylesheet" />

</head>

<body>
    @RenderBody()

    <h1>Inventario De Repuestos Web</h1>
    <h2><a href="http://tutorialzine.com/2010/06/css3-minimalistic-navigation-menu/">Read the tutorial on Tutorialzine &raquo;</a></h2>

    <div id="main">

        <ul id="navigationMenu">
            <li>
                <a class="home" href="~/Home/Index">
                    <span>Piezas de Repuesto</span>
                </a>
            </li>

            <li>
                <a class="about" href="#">
                    <span>Mantenimiento</span>
                </a>
            </li>

            <li>
                <a class="services" href="#">
                    <span>Accesorios Interior</span>
                </a>
            </li>

            <li>
                <a class="portfolio" href="#">
                    <span>Accesorios Exterior</span>
                </a>
            </li>

            <li>
                <a class="contact" href="#">
                    <span>Contactanos</span>
                </a>
            </li>
        </ul>

    </div>


    <p class="note">View in a Webkit browser (Safari, Chrome) for the full effect.</p>

</body>
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)

</html>

